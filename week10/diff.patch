+	cfg.Producer.Return.Successes = true
+	producer, err := sarama.NewSyncProducer(addr, cfg)
+	cfg.Producer.Partitioner = sarama.NewRoundRobinPartitioner
+	//cfg.Producer.Partitioner = sarama.NewRandomPartitioner
+	//cfg.Producer.Partitioner = sarama.NewHashPartitioner
+	//cfg.Producer.Partitioner = sarama.NewManualPartitioner
+	//cfg.Producer.Partitioner = sarama.NewConsistentCRCHashPartitioner
+	//cfg.Producer.Partitioner = sarama.NewCustomPartitioner()
+	assert.NoError(t, err)
+	for i := 0; i < 100; i++ {
+		_, _, err = producer.SendMessage(&sarama.ProducerMessage{
+			Topic: "test_topic",
+			Value: sarama.StringEncoder(""),
+			// 
+			Headers: []sarama.RecordHeader{
+				{
+					Key:   []byte("key1"),
+					Value: []byte("value1"),
+				},
+			},
+			Metadata: " metadata",
+		})
+	}
+}
+
+func TestAsyncProducer(t *testing.T) {
+	cfg := sarama.NewConfig()
+	cfg.Producer.Return.Successes = true
+	cfg.Producer.Return.Errors = true
+	producer, err := sarama.NewAsyncProducer(addr, cfg)
+	assert.NoError(t, err)
+	msgs := producer.Input()
+	msgs <- &sarama.ProducerMessage{
+		Topic: "test_topic",
+		Value: sarama.StringEncoder(""),
+		// 
+		Headers: []sarama.RecordHeader{
+			{
+				Key:   []byte("key1"),
+				Value: []byte("value1"),
+			},
+		},
+		Metadata: " metadata",
+	}
+
+	select {
+	case msg := <-producer.Successes():
+		t.Log("", string(msg.Value.(sarama.StringEncoder)))
+	case err := <-producer.Errors():
+		t.Log("", err.Err, err.Msg)
+	}
+}
